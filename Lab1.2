#include <stdio.h>
#include <stdlib.h>

void GetSet(int arr[], int *n) {
    printf("Enter number of members: ");
    scanf("%d", n);

    for(int i = 0; i < *n; i++) {
        printf("Enter members %d: ", i + 1);
        scanf("%d", &arr[i]);
    }
}

int main() {
    int data[100], num;
    GetSet(data, &num);

    printf("\nSet = { ");
    for(int i = 0; i < num; i++) printf("%d ", data[i]);
    printf("}\n");

    return 0;
}
