#include <stdio.h>

int *GetSet(int *n) {
    static int arr[100];
    
    printf("Enter number of member: ");
    scanf("%d", n);
    
    printf("Enter %d members: ", *n);
    for (int i = 0; i < *n; i++) {
        scanf("%d", &arr[i]);
    }
    
    return arr;
}

int main() {
    int *data, num;
    data = GetSet(&num);
    
    printf("Set: ");
    for (int i = 0; i < num; i++) {
        printf("%d ", data[i]);
    }
    printf("\n");
    
    return 0;
}
